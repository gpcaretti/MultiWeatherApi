<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Gianpiero Caretti</Authors>
    <Company>GP software engineering</Company>
    <Description>API abstraction of OpenWeatherMap and DarkSky API</Description>
    <PackageTags>Weather OpenWeatherMap DarkSky netstandard c#</PackageTags>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryType>GitHub</RepositoryType>
		<ApplicationIcon>packageIcon.ico</ApplicationIcon>
		<PackageIcon>packageIcon.png</PackageIcon>
	</PropertyGroup>

  <ItemGroup>
    <None Include="..\packageIcon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
